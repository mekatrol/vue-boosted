{
  "name": "vue-boosted",
  "version": "0.0.2",
  "description": "Vue.js (TypeScript) accessible (a11y), testable (UI automation) Vue component and utilities library.",
  "main": "index.ts",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc && webpack --mode=production",
    "test": "npm run test",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mekatrol/vue-boosted.git"
  },
  "keywords": [
    "Vue.js",
    "TypeScript",
    "accessible",
    "WCAG",
    "a11y",
    "automation",
    "secure",
    "safe"
  ],
  "author": "Paul Wojcik",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/mekatrol/vue-boosted/issues"
  },
  "homepage": "https://github.com/mekatrol/vue-boosted#readme",
  "dependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dts-bundle": "^0.7.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "files": [
    "dist"
  ]
}
